                  },
                  "Tile": {
                    "Query": "TeslaSuperchargerData_CL | where Status_s =~ \"PERMIT\" or Status_s =~ \"CONSTRUCTION\" | distinct Name_s, StallCount_d, Country_s | sort by StallCount_d desc | limit 10 | summarize sum(StallCount_d)",
