                    "NavigationSelect": {}
                  },
                  "List": {
                    "Query": "TeslaSuperchargerData_CL | sort by StallCount_d desc | where Status_s =~ \"OPEN\"  | distinct Name_s, StallCount_d, Country_s | sort by StallCount_d desc | limit 10  ",
